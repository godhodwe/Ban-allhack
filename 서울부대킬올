local EffectFolders = {
    ChargeLevel = 0,
    HitEffectFolder = ReplicatedStorage:WaitForChild("Miscs"):WaitForChild("GunVisualEffects"):WaitForChild("Common"):WaitForChild("HitEffect"),
    MuzzleFolder = ReplicatedStorage:WaitForChild("Miscs"):WaitForChild("GunVisualEffects"):WaitForChild("Common"):WaitForChild("MuzzleEffect"),
    BloodEffectFolder = ReplicatedStorage:WaitForChild("Miscs"):WaitForChild("GunVisualEffects"):WaitForChild("Common"):WaitForChild("BloodEffect"),
    ExplosionEffectFolder = ReplicatedStorage:WaitForChild("Miscs"):WaitForChild("GunVisualEffects"):WaitForChild("Common"):WaitForChild("ExplosionEffect"),
    GoreEffect = ReplicatedStorage:WaitForChild("Miscs"):WaitForChild("GunVisualEffects"):WaitForChild("Common"):WaitForChild("GoreEffect")
}

local DamageAmount = 100 

local function InflictDamageToTarget(targetPlayer)
    local character = targetPlayer.Character
    if not character or not character.Parent then return end

    local humanoid = character:FindFirstChild("Humanoid")
    local rootPart = character:FindFirstChild("HumanoidRootPart")
    
    if not humanoid or humanoid.Health <= 0 or not rootPart then return end

    local targetPart = character:FindFirstChild("Left Arm") or rootPart
    
    local args = {
        "Gun",         
        AttackerGun,   
        WeaponStats,   
        humanoid,      
        rootPart,      
        targetPart,    
        EffectFolders, 
        DamageAmount   
    }

    pcall(InflictTarget.InvokeServer, InflictTarget, unpack(args))
end

while true do
    for _, player in Players:GetPlayers() do
        if player ~= LocalPlayer then
            InflictDamageToTarget(player)
        end
    end

    task.wait(0.1)
end
